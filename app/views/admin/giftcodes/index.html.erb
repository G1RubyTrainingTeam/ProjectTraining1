<div class="row">
  <div class="col-lg-12">
    <h2 class="page-header title">All Giftcode</h2>
  </div>
</div>
<hr>
<table class="table table-hover table-bordered datatable">
  <thead class="text-center">
    <tr>
      <th class="col-md-2">index</th>
      <th class="col-md-5">code</th>
      <th class="col-md-5">value</th>
    </tr>
  </thead>
  <tbody>
    <% @giftcode.each_with_index do |giftcode, index| %>
      <tr class="odd gradeX">
        <td><%= index + 1 %>
        <td><%= giftcode.code %></td>
        <td><%= giftcode.value %></td>
      </tr>
    <% end %>
  </tbody>
</table>
